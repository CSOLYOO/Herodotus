<ConfigSection>
    <IfCondition condition=':= dimension.id = -1'>
         <Cloud name='IronOre' inherits='PresetStrategicCloud'>
		 <OreBlock block='minecraft:quartz_ore' />
            <Biome name='.*'  />
            <Setting name='DistributionFrequency' avg=':= 10 * _default_ ' range=':= 6 * _default_ ' type='normal' scaleTo='base' />
            <Setting name='CloudRadius' avg=':= 0.6 * _default_ ' range=':= 1 * _default_ ' type='normal' />
            <Setting name='CloudThickness' avg=':= 0.8 * _default_ ' range=':= 0.8 * _default_ ' type='normal' scaleTo='base' />
            <Setting name='CloudHeight' avg=':= 20/50' range=':= 20/50' type='normal'/>
            <Setting name='ParentRangeLimit' avg=':= _default_ ' range=':= _default_ ' type='normal' />
            <Setting name='CloudSizeNoise' avg=':= _default_ ' range=':= _default_ ' type='normal' />
            <Setting name='CloudInclination' avg=':= _default_ ' range=':= _default_ ' type='normal' />
            <Setting name='OreDensity' avg=':= 5 * _default_ ' range=':= 4 * _default_ ' type='normal' scaleTo='base' />
            <Setting name='OreVolumeNoiseCutoff' avg=':= 0.5 * _default_ ' range=':= 0.5 * _default_ ' type='normal' />
            <Setting name='OreRadiusMult' avg=':= _default_ ' range=':= _default_ ' type='normal' />
        </Cloud>
 </IfCondition>

</ConfigSection>
